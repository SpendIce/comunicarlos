meta {
  name: Registro Solicitante
  type: http
  seq: 1
}

post {
  url: {{base_url}}/api/v1/auth/registro
  body: json
  auth: inherit
}

headers {
  Content-Type: application/json
}

body:json {
  {
    "nombre": "Juan Perez",
    "email": "{{email_solicitante}}",
    "password": "{{password_comun}}",
    "tipo_usuario": "SOLICITANTE",
    "servicios_suscritos": [
      {
        "tipo": "INTERNET_BANDA_ANCHA",
        "numero_servicio": "INT-55501"
      }
    ]
  }
}

assert {
  res.status: eq 201
}
